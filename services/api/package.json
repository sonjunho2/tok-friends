{
  "name": "tokfriends-api",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@10.14.0",
  "engines": {
    "node": "20.x"
  },
  "scripts": {
    "start": "node dist/main.js",
    "start:prod": "node dist/main.js",
    "start:dev": "nest start --watch",
    "build": "nest build",
    "prebuild": "prisma generate",
    "postinstall": "prisma generate",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint \"src/**/*.ts\"",
    "prisma:studio": "prisma studio",
    "prisma:generate": "prisma generate",
    "prisma:push": "prisma db push",
    "prisma:seed": "ts-node --swc prisma/seed.ts",
    "prisma:migrate": "prisma migrate dev",
    "prisma:migrate:deploy": "prisma migrate deploy",
    "prestart:dev": "prisma generate",
    "heroku-build": "npm i -g pnpm && pnpm config set allow-scripts @prisma/client@*,prisma,argon2 --location=project && pnpm i --frozen-lockfile || pnpm i --no-frozen-lockfile && pnpm prisma:generate && pnpm prisma:push && pnpm build"
  },
  "dependencies": {
    "@aws-sdk/client-rekognition": "3.864.0",
    "@aws-sdk/client-s3": "3.864.0",
    "@aws-sdk/client-translate": "3.864.0",
    "@aws-sdk/s3-request-presigner": "3.864.0",
    "@nestjs/common": "10.4.20",
    "@nestjs/config": "3.3.0",
    "@nestjs/core": "10.4.20",
    "@nestjs/passport": "10.0.3",
    "@nestjs/platform-express": "10.4.20",
    "@nestjs/platform-socket.io": "10.4.20",
    "@nestjs/swagger": "7.4.2",
    "@nestjs/websockets": "10.4.20",
    "@prisma/client": "5.22.0",
    "argon2": "0.40.3",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.2",
    "express-basic-auth": "1.2.1",
    "firebase-admin": "12.7.0",
    "helmet": "7.2.0",
    "jose": "5.10.0",
    "jsonwebtoken": "9.0.2",
    "nestjs-prisma": "0.23.0",
    "node-fetch": "3.3.2",
    "passport": "0.7.0",
    "passport-jwt": "4.0.1",
    "reflect-metadata": "0.2.2",
    "rimraf": "5.0.10",
    "rxjs": "7.8.2",
    "socket.io": "4.8.1",
    "swagger-ui-express": "5.0.1"
  },
  "devDependencies": {
    "@nestjs/cli": "10.4.9",
    "@nestjs/schematics": "10.2.3",
    "@nestjs/testing": "10.4.20",
    "@swc/core": "1.4.2",
    "@swc/helpers": "0.5.6",
    "@types/express": "4.17.23",
    "@types/jsonwebtoken": "9.0.10",
    "@types/node": "20.19.11",
    "@typescript-eslint/eslint-plugin": "7.18.0",
    "@typescript-eslint/parser": "7.18.0",
    "eslint": "8.57.1",
    "eslint-config-prettier": "9.1.2",
    "eslint-plugin-import": "2.32.0",
    "prettier": "3.6.2",
    "prisma": "5.22.0",
    "ts-node": "10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.9.2"
  }
}
